{
  "name": "@jcbhmr/exec",
  "version": "1.0.0",
  "description": "üêß Ergonomic exec(3) for JavaScript",
  "keywords": [
    "exec",
    "execve"
  ],
  "homepage": "https://github.com/jcbhmr/exec.js#readme",
  "bugs": {
    "url": "https://github.com/jcbhmr/exec.js/issues"
  },
  "license": "MIT",
  "author": "Jacob Hummer <jcbhmr@outlook.com> (https://jcbhmr.com)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcbhmr/exec.js.git"
  },
  "workspaces": [
    "./packages/[!@]*",
    "./packages/@*/*"
  ],
  "files": [
    "dist/"
  ],
  "os": [
    "netbsd",
    "os390",
    "darwin",
    "sunos",
    "freebsd",
    "openbsd",
    "linux",
    "android",
    "os400",
    "ios",
    "openharmony"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "imports": {
    "#process-execve": {
      "deno": {
        "development:@jcbhmr/exec": "./src/process-execve.deno.ts",
        "default": "./dist/process-execve.deno.js"
      },
      "bun": {
        "development:@jcbhmr/exec": "./src/process-execve.bun.ts",
        "default": "./dist/process-execve.bun.js"
      },
      "node": {
        "development:@jcbhmr/exec": "./src/process-execve.node.ts",
        "default": "./dist/process-execve.node.js"
      }
    }
  },
  "exports": {
    ".": {
      "development:@jcbhmr/exec": "./src/index.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "tsdown",
    "format": "oxfmt",
    "lint": "oxlint --fix",
    "check": "tsc",
    "test": "node --conditions development:@jcbhmr/exec --test",
    "prepublishOnly": "test -n \"$(git status --porcelain)\""
  },
  "dependencies": {
    "which": "^6.0.0"
  },
  "devDependencies": {
    "@tsconfig/bases": "^1.0.17",
    "@types/bun": "^1.3.6",
    "@types/deno": "^2.5.0",
    "@types/node": "^25.0.9",
    "@types/which": "^3.0.4",
    "bun": "^1.3.6",
    "deno": "^2.6.5",
    "execa": "^9.6.1",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0",
    "publint": "^0.3.17",
    "tsdown": "^0.20.0-beta.3",
    "typescript": "^5.9.3"
  },
  "devEngines": {
    "packageManager": {
      "name": "npm",
      "version": "^11"
    },
    "runtime": {
      "name": "node",
      "version": "^24"
    }
  },
  "engines": {
    "bun": ">=1",
    "deno": ">=2",
    "node": ">=25 || ^24 || ^22 || ^20"
  }
}
